<%= @event.title %>
<%= @event.description %>
<%= @event.date %>
<%= "Created by #{@event.creator.username}" %>


<h2>Attendees</h2>
<% @event.attendees.reverse.uniq.each do |attendee| %>
    <%= attendee.username %>
<% end %>

<% unless @event.creator == current_user %>
    <%= render partial:"join" %>
<% end %>